using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
namespace stringoperation
{
 public class Class1
 {
 static void Main(string[] args)
 {
 String a, b,c;
Console.Write("Enter 1st string\t");
a = Console.ReadLine();
Console.Write("Enter 2nd string\t");
b = Console.ReadLine();
   c=a+b;
   Console.Write("Concatinated string is = "+c);
 }
 }
}
